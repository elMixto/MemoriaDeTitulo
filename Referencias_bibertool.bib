  ISSN = {1557-9964},
  JOURNALTITLE = {IEEE/ACM transactions on computational biology and bioinformatics},
  KEYWORDS = {Algorithms,Computational Biology,Gene Expression Profiling,Humans,Protein Interaction Mapping,Sequence Alignment,Yeasts},
  NUMBER = {6},
  PAGES = {1446--1458},
  SHORTTITLE = {Triangular {Alignment} ({TAME})},
  TITLE = {Triangular {Alignment} ({TAME}): {A} {Tensor}-{Based} {Approach} for {Higher}-{Order} {Network} {Alignment}},
