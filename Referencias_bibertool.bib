  ISSN = {1573-773X},
  JOURNALTITLE = {Neural Processing Letters},
  KEYWORDS = {Genetic algorithm,Machine learning,Combinatorial optimisation,Evolutionary computation,Multidimensional knapsack problem},
  NUMBER = {2},
  PAGES = {871--890},
  TITLE = {Application of {Supervised} {Machine} {Learning} {Methods} on the {Multidimensional} {Knapsack} {Problem}},
  URLDATE = {2023-07-26},
  VOLUME = {54},
}

